{{page-title "Students"}}
<div class="p-5">
  <div class="mb-3">
    <h2>Current courses:</h2>
  </div>
  <table class="table">
    <thead>
    <tr>
      <th>Name</th>
      <th>Last Name</th>
      <th>Enrolled Course </th>
    </tr>
    </thead>
    <tbody>
    {{#each this.allStudents as |student|}}
      <tr>
        <td>{{student.name}}</td>
        <td>{{student.lastName}}</td>
        <td>{{'Empty'}}</td>
      </tr>
    {{/each}}
    </tbody>
  </table>
</div>
{{outlet}}